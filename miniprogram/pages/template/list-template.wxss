/* miniprogram/pages/list-template/list-template.wxss */
.tempContainer{
  display: flex;
  flex-direction: column;
}
.tempContainer .userInfo{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.tempContainer .userInfo .avatar{
  margin: 20rpx 40rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
}
.tempContainer .article{
  overflow: hidden;
}
.tempContainer .article-title{
  margin: 0 30rpx;
  font-size: 30rpx;
  font-weight: 700;
}

.tempContainer .article-image{
  margin-top: 20rpx;
  width: 100%;
  height: 460rpx;
}

.tempContainer .article-content{
  font-size: 32rpx;
  padding: 0 50rpx;
}
.tempContainer .collection{
  display: flex;
  align-items: center;
  padding-left: 10rpx;
}
.tempContainer .collection my-icon{
  padding: 20rpx 20rpx;
}